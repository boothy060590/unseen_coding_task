includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:
    level: 5
    paths:
        - app/
        - database/
        - routes/
    
    # Ignore errors in vendor and generated files
    excludePaths:
        - bootstrap/cache/
        - storage/
        - vendor/
        - node_modules/
        
    # Laravel specific ignores
    reportUnmatchedIgnoredErrors: false